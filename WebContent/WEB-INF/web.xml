<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">

	<display-name>ConferenceRoomBooking</display-name>
	<servlet>
		<servlet-name>add</servlet-name>
		<servlet-class>booking.Add</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>signup</servlet-name>
		<servlet-class>booking.Signup</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>rooms</servlet-name>
		<servlet-class>booking.Rooms</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>book</servlet-name>
		<servlet-class>booking.Book</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>validate</servlet-name>
		<servlet-class>booking.Validate</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>signin</servlet-name>
		<servlet-class>booking.Signin</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>welcome</servlet-name>
		<servlet-class>booking.Welcome</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>welcome</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>signin</servlet-name>
		<url-pattern>/Signin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>validate</servlet-name>
		<url-pattern>/login/validate</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>book</servlet-name>
		<url-pattern>/book</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>rooms</servlet-name>
		<url-pattern>/Rooms</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>signup</servlet-name>
		<url-pattern>/Signup</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>add</servlet-name>
		<url-pattern>/Add</url-pattern>
	</servlet-mapping>
	
















</web-app>